<div class="flex flex-col absolute bg-slate-50 z-50 w-full border rounded mt-1">
    {% for project in projects %}
        <a href="{{ path('app_project_show',{slug:project.slug}) }}" class="p-2 hover:bg-blue-100">
            {{ project.name }}
        </a>
    {% else %}
        <div class="p-2">
            No results found
        </div>
    {% endfor %}
</div>